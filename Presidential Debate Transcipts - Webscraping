    "war_count_df = pd.DataFrame(columns = df.columns, index= ['war count'])\n",
    "x = 0\n",
    "for a in speech_array:\n",
    "    content = requests.get(a).content\n",
    "    cont_soup = bs.BeautifulSoup(content, 'html.parser')\n",
    "    speech = str(cont_soup.find(id = 'content-sm'))\n",
    "    counts = Counter(re.findall(r\"[\\w']+\" , speech.lower()))\n",
    "    count_war = counts['war']\n",
    "    war_count_df.iloc[:,x] = [count_war]\n",
    "    x = x+1\n",
    "df = df.append(war_count_df)\n",
    "df\n"
