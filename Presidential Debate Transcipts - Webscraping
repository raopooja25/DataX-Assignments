   "source": [
    "x=0\n",
    "for a in speech_array:\n",
    "    content = requests.get(a).content\n",
    "    cont_soup = bs.BeautifulSoup(content, 'html.parser')\n",
    "    speech = str(cont_soup.find(id = 'content-sm'))\n",
    "    speech = re.sub('<.*?>','',speech)\n",
    "    speech = re.sub('\\n','',speech)\n",
    "    len_transcript = len(speech)\n",
    "    df.iloc[0,x] = len_transcript\n",
    "    x = x+1\n",
    "df"
   ]
