    "all_links = soup.find_all('a')\n",
    "speech_array = []\n",
    "title_array = []\n",
    "for a in all_links:\n",
    "    title = a.text\n",
    "    if 'First' in title and 'Presidential' in title and 'Debate' in title:\n",
    "        if '1988' in title or '1984' in title or '1976' in title or '1960' in title:\n",
    "            links = a.get('href')\n",
    "            final_url = urljoin('https://www.debates.org/voter-education/debate-transcripts',links)\n",
    "            speech_array.append(final_url)\n",
    "            title_array.append(title)\n",
    "            print(title, final_url)\n"
