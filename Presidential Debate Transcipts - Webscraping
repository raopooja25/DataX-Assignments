   "source": [
    "def count_dataframe(names_list):\n",
    "    datas = Counter(names_list)\n",
    "    authors = pd.DataFrame(columns= ['Counts'], data=list(datas.values()),index = list(datas.keys())).sort_values('Counts', ascending = False)\n",
    "    authors.index.name = 'Authors'\n",
    "    return authors\n",
    "count_dataframe(fifth_line_names)"
