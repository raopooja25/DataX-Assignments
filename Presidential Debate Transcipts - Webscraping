   "source": [
    "df1 = pd.DataFrame(columns = df.columns, index = ['Most Commmon Word', 'Most Common Word Count'])\n",
    "most_common_word = []\n",
    "most_common_count = []\n",
    "x = 0\n",
    "for a in speech_array:\n",
    "    content = requests.get(a).content\n",
    "    cont_soup = bs.BeautifulSoup(content, 'html.parser')\n",
    "    speech = str(cont_soup.find(id = 'content-sm'))\n",
    "    speech = re.sub('<.*?>','',speech)\n",
    "    speech = re.sub('\\n','',speech)\n",
    "    counts = Counter(re.findall(r\"[\\w']+\" , speech.lower()))\n",
    "    words = list(counts.keys())\n",
    "    count2 = list(counts.values())\n",
    "    df2 = pd.DataFrame(columns = ['count'], index = words, data = count2)\n",
    "    df2 = df2.sort_values('count', ascending = False)\n",
    "    most_common_word.append(df2.index[0])\n",
    "    most_common_count.append(df2.iloc[0,0])\n",
    "    x = x+1\n",
    "for column_number in np.arange(len(df1.columns)):\n",
    "    df1.iloc[0, column_number] = most_common_word[column_number]\n",
    "    df1.iloc[1, column_number] = most_common_count[column_number]\n",
    "\n",
    "df = df.append(df1)\n",
    "df"
   ]
  },
  {
